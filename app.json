{
  "expo": {
    "name": "temp-app",
    "slug": "temp-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "tempapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "extra": {
      "authRedirect": "tempapp://auth/callback"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
